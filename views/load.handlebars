{{!-- <header>Welcome to our Forum!</header> --}}


<img class="backgroundImg" src="https://static1.visitestonia.com/images/2890339/Estonian_forest_reasons_to_visit_Sven_Zacek.jpg">
<div class="container">
    <section id="intro" class="wrapper">
        <h1 class="header headerx name">Welcome to Carved In Stone</h1>

        <div class="divider byline"></div>
    </section>
</div>


<div class="container">
    <section id="explosion" class="wrapper">
        <h1 class="explosion-byline">Recent Posts</div></h3>
        
    </section>
</div>

<div class="container">
    <section id="links" class="wrapper ">
        
        <div class="container links-byline">
            
                {{#each examples}}
                <div class="row postBow">
                    <a class="postHolder" href="example/{{this.id}}">
                        <div class="card blue-grey darken-1 postHolder">
                            <div class="card-content white-text">
                                <span class="card-title">{{this.text}}</span>
                            </div>
                        </div>
                    </a>
                </div>
                {{/each}}
        
        </div>
        
    </section>
    
</div>
<img class ="logo toTop" src="https://cdn3.iconfinder.com/data/icons/sympletts-free-sampler/128/diamond-512.png">



